{
    "name": "training/unittest",
    "description": "BSD traning DEV unit test",
    "keywords": ["unit test",  "unit test"],
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "php": "^7.2|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "orchestra/testbench": "^5.20"
    },

    "authors": [
        {
            "name": "QuanNH",
            "email": "quannh@runsystem.net"
        }
    ],

    "extra": {
        "laravel": {
            "providers": [
                "BsdTraning\\UnitTest\\ServiceProvider"
            ]

        }
    },

    "autoload": {
        "psr-4": {
            "BsdTraning\\UnitTest\\": "src/",
            "BsdTraning\\UnitTest\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BsdTraning\\Tests\\": "tests/"
        }
    }
}
